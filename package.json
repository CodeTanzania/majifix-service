{
  "name": "@codetanzania/majifix-service",
  "version": "1.2.0",
  "description": "A representation of an acceptable service (request types)(e.g Water Leakage) offered(or handled) by a specific jurisdiction",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ rollup.config.js",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test mocha --timeout 8000 --exit --require @babel/register test/**/*.spec.js",
    "dev": "npm run seed && npm run start",
    "start": "BASE_PATH=./examples node ./examples/app.js",
    "seed": "BASE_PATH=./examples node ./examples/seed.js",
    "docs": "doxdox 'lib/**/*.js' -p package.json -l markdown -o DOCUMENTATION.md",
    "cmt": "git add -A && git-cz",
    "commit": "git-cz",
    "commit:all": "git add -A && git-cz",
    "changelog": "changelog"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeTanzania/majifix-service.git"
  },
  "keywords": [
    "codetanzania",
    "majifix",
    "majifix-module",
    "service",
    "open311"
  ],
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    },
    {
      "name": "benMaruchu",
      "email": "benmaruchu@gmail.com",
      "url": "https://github.com/BenMaruchu"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeTanzania/majifix-service/issues",
    "email": "lallyelias87@gmail.com"
  },
  "homepage": "https://github.com/CodeTanzania/majifix-service",
  "sandbox": {
    "url": "https://majifix-service.herokuapp.com/v1/services"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/node": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.5.5",
    "@benmaruchu/faker": "4.3.0",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@commitlint/travis-cli": "8.1.0",
    "@lykmapipo/mongoose-test-helpers": "0.7.16",
    "chai": "4.2.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "doxdox": "3.0.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-mocha": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "generate-changelog": "1.8.0",
    "handlebars": "4.1.2",
    "husky": "3.0.2",
    "jshint-stylish": "2.2.1",
    "lint-staged": "9.2.1",
    "lodash.template": "4.5.0",
    "mocha": "6.2.0",
    "mongoose": "5.6.8",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "rollup": "1.19.4",
    "sinon": "7.4.1",
    "sinon-chai": "3.3.0",
    "sinon-mongoose": "2.3.0",
    "supertest": "4.0.2"
  },
  "dependencies": {
    "@codetanzania/majifix-common": ">=0.13.0",
    "@codetanzania/majifix-jurisdiction": ">=1.5.5",
    "@codetanzania/majifix-priority": ">=1.4.5",
    "@codetanzania/majifix-service-group": ">=1.2.6",
    "@lykmapipo/common": ">=0.24.2",
    "@lykmapipo/env": ">=0.11.1",
    "@lykmapipo/express-common": ">=0.17.9",
    "@lykmapipo/mongoose-common": ">=0.25.9",
    "@lykmapipo/predefine": ">=0.6.0",
    "async": ">=3.1.0",
    "express": ">=4.17.1",
    "lodash": ">=4.17.15",
    "moment": ">=2.24.0",
    "mongoose-locale-schema": ">=0.3.4",
    "mongoose-rest-actions": ">=0.28.6",
    "randomcolor": ">=0.5.4",
    "winston": ">=3.2.1"
  },
  "peerDependencies": {
    "mongoose": ">=5.6.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ],
    "*.{md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.6.0"
  }
}
